{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
    "version": "0.2",
    "files": [
        "**/*"
    ],
    "enabledLanguageIds": [
        "markdown",
        "typescript"
    ],
    "ignorePaths": [
        "node_modules",
        "coverage",
        "data/**",
        "doc/api/**",
        "src/sandbox/**",
        "package-lock.json",
        "lib",
        "*.log",
        "*.tgz"
    ],
    "words": [
        "apidoc",
        "kayahr",
        "Klingemann",
        "Reimer",
        "RGBA",
        "stackblur"
    ],
    "language": "en",
    "enableFiletypes": [
        "json"
    ]
}
